     t_proyecto."lugar" AS t_proyecto_lugar,
     t_proyecto."calificacion" AS t_proyecto_calificacion,
     t_proyecto."codigo_proyecto" AS t_proyecto_codigo_proyecto,
     t_proyecto."estado_proyecto" AS t_proyecto_estado_proyecto,
